{
  "name": "terms-and-conditions-gui",
  "version": "0.1.0",
  "description": "An interface for FDK terms and conditions.",
  "private": true,
  "engines": {
    "node": ">= 12.16.3"
  },
  "scripts": {
    "start": "run-s start:webpack",
    "start:webpack": "webpack-dev-server --hot --inline -r @babel/register --config webpack/dev.config.js",
    "build:dev": "run-s build:webpack:dev",
    "build:prod": "run-s build:webpack:prod",
    "build:webpack:dev": "webpack -r @babel/register --config webpack/dev.config.js",
    "build:webpack:prod": "webpack -r @babel/register --config webpack/prod.config.js",
    "build:webpack:stats": "webpack -r @babel/register --config webpack/stats.config.js",
    "type:check": "tsc",
    "lint:project": "eslint --ext tsx,ts,jsx,js,json,md,html ./",
    "lint:project:fix": "eslint --ext tsx,ts,jsx,js,json,md,html ./ --fix",
    "lint:staged:fix": "eslint --ext tsx,ts,jsx,js,json,md,html --fix",
    "test": "run-s clean:coverage test:jest",
    "test:verbose": "run-s clean:coverage test:jest:verbose",
    "test:coverage": "run-s clean:coverage test:jest:coverage",
    "test:jest": "jest --passWithNoTests",
    "test:jest:verbose": "jest --verbose",
    "test:jest:coverage": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll",
    "test:watch:verbose": "jest --watch --verbose",
    "test:watch:all:verbose": "jest --watchAll --verbose",
    "clean:coverage": "rm -rf coverage",
    "bundle:analyse": "run-s build:webpack:stats",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:Informasjonsforvaltning/terms-and-conditions-gui.git"
  },
  "author": {
    "name": "Alexander Zakharov (azaxarov)",
    "email": "alexander.zakharov@capgemini.com"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-helmet": "^6.0.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "@types/yup": "^0.29.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.0.5",
    "cz-conventional-changelog": "^3.1.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-styled-components": "^7.0.2",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^10.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-svg-loader": "^3.0.3",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
